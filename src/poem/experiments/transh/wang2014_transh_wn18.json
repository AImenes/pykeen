{
  "metadata": {
    "title": "Learn WN18 Data Set with TransH as described by Wang et al., 2014"
  },
  "pipeline": {
    "data_set": "wn18",
    "model": "TransH",
    "model_kwargs": {
      "embedding_dim": 50,
      "regularization_weight": 0.25,
      "scoring_fct_norm": 2
    },
    "optimizer": "SGD",
    "optimizer_kwargs": {
      "lr": 0.01
    },
    "criterion": "MarginRankingLoss",
    "criterion_kwargs": {
      "margin": 1
    },
    "early_stopping": true,
    "early_stopping_kwargs": {
      "metric": "mean_rank",
      "frequency": 50
    },
    "training_loop": "OWA",
    "training_kwargs": {
      "num_epochs": 500,
      "batch_size": 4800,
      "negative_sampler_cls": "BernoulliNegativeSampler"
    }
  }
}
