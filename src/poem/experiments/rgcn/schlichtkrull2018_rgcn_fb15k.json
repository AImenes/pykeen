{
  "metadata": {
    "title": "Learn FB15k Data Set with R-GCN as described by Schlichtkrull et al., 2018",
    "comments": ""

  },
  "pipeline": {
    "data_set": "fb15k",
    "model": "R-GCN",
    "model_kwargs": {
      "embedding_dim": 500,
      "num_bases_or_blocks": 100,
      "decomposition": "block"
    },
    "optimizer": "Adagrad",
    "optimizer_kwargs": {
      "lr": 0.1
    },
    "criterion": "SoftplusLoss",
    "early_stopping": true,
    "early_stopping_kwargs": {
      "metric": "mean_rank",
      "frequency": 50
    },
    "training_loop": "OWA",
    "training_kwargs": {
      "num_epochs": 6000,
      "batch_size": 30000,
      "negative_sampler_cls": "BasicNegativeSampler",
      "num_negs_per_pos": 10
    }
  }
}
