{
  "metadata": {
    "title": "Learn FB15k-237 Data Set with TransE as described by Nguyen et al., 2018",
    "commenrs": "TransE was used to initialize the emebddings. Could not find batch size and number of epochs."
  },
  "pipeline": {
    "data_set": "wn18rr",
    "model": "TransE",
    "model_kwargs": {
      "embedding_dim": 50
    },
    "optimizer": "SGD",
    "optimizer_kwargs": {
      "lr": 0.0005
    },
    "criterion": "MarginRankingLoss",
    "criterion_kwargs": {
      "margin": 5
    },
    "early_stopping": true,
    "early_stopping_kwargs": {
      "metric": "mean_rank",
      "frequency": 50
    },
    "training_loop": "OWA",
    "training_kwargs": {
      "num_epochs": 3000,
      "batch_size": 256,
      "negative_sampler_cls": "BernoulliNegativeSampler",
      "num_negs_per_pos": 1
    }
  }
}