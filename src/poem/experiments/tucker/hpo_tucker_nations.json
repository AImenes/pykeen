{
  "metadata": {
    "title": "HPO Over Nations for TuckER"
  },
  "pipeline": {
    "dataset": "nations",
    "dataset_kwargs": {
      "create_inverse_triples": true
    },
    "model": "TuckER",
    "model_kwargs_ranges": {
      "embedding_dim": {
        "type": "int",
        "low": 5,
        "high": 15
      },
       "relation_dim": {
        "type": "int",
        "low": 5,
        "high": 15
      },
      "dropout_0": {
        "type": "float",
        "low": 0.01,
        "high": 0.3,
        "scale": "log"
      },
       "dropout_1": {
        "type": "float",
        "low": 0.01,
        "high": 0.3,
        "scale": "log"
      },
       "dropout_2": {
        "type": "float",
        "low": 0.01,
        "high": 0.3,
        "scale": "log"
      }
    },
    "optimizer": "Adam",
    "optimizer_kwargs": {
    },
    "optimizer_kwargs_ranges": {
      "lr": {
        "type": "float",
        "low": 0.001,
        "high": 0.1,
        "scale": "log"
      }
    },
    "loss": "BCEAfterSigmoid",
    "loss_kwargs": {
    },
    "training_loop": "LCWA",
    "training_kwargs": {
      "batch_size": 128
    },
    "training_kwargs_ranges": {
      "num_epochs": {
        "type": "int",
        "low": 40,
        "high": 100,
        "q": 10
      }
    },
    "evaluator_kwargs": {
      "filtered": true
    },
    "evaluation_kwargs": {
      "batch_size": 128
    }
  },
  "optuna": {
    "n_trials": 10,
    "timeout": 30,
    "metric": "hits@10",
    "direction": "maximize",
    "sampler": "TPE"
  }
}
