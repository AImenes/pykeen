{
  "metadata": {
    "title": "Learn WN18 Data Set with KG2E as described by He et al., 2015"
  },
  "pipeline": {
    "data_set": "wn18",
    "model": "KG2E",
    "model_kwargs": {
      "embedding_dim": 50,
      "c_min": 0.03,
      "c_max": 3.0
    },
    "optimizer": "SGD",
    "criterion": "MarginRankingLoss",
    "criterion_kwargs": {
      "margin": 4.0
    },
    "early_stopping": true,
    "early_stopping_kwargs": {
      "metric": "mean_rank",
      "frequency": 50
    },
    "training_loop": "OWA",
    "training_kwargs": {
      "num_epochs": 500,
      "batch_size": 20,
      "negative_sampler_cls": "BernoulliNegativeSampler"
    }
  }
}