{
  "metadata": {
    "title": "Learn WN18RR Data Set with ConvKB as described by Nguyen et al., 2018"
  },
  "pipeline": {
    "data_set": "wn18rr",
    "model": "ConvKB",
    "model_kwargs": {
      "embedding_dim": 50,
      "num_filters": 500,
      "regularization_weight": 0.001
    },
    "optimizer": "Adam",
    "optimizer_kwargs": {
      "lr": 0.0001
    },
    "loss": "SoftplusLoss",
    "training_loop": "OWA",
    "negative_sampler": "bernoulli",
    "negative_sampler_kwargs": {
      "num_negs_per_pos": 1
    },
    "training_kwargs": {
      "num_epochs": 200,
      "batch_size": 256
    }
  }
}
