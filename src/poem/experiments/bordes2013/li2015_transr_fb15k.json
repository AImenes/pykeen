{
  "metadata": {
    "title": "Learn FB15K Data Set with TransR as described by Li et al., 2015"
  },
  "pipeline": {
    "data_set": "fb15k",
    "model": "TransR",
    "model_kwargs": {
      "embedding_dim": 50,
      "relation_dim": 50,
      "scoring_fct_norm": 2
    },
    "optimizer": "SGD",
    "optimizer_kwargs": {
      "lr": 0.001
    },
    "criterion": "MarginRankingLoss",
    "criterion_kwargs": {
      "margin": 1
    },
    "early_stopping": true,
    "early_stopping_kwargs": {
      "metric": "mean_rank",
      "frequency": 50
    },
    "training_loop": "OWA",
    "training_kwargs": {
      "num_epochs": 500,
      "batch_size": 4800,
      "negative_sampler_cls": "BernoulliNegativeSampler"
    }
  }
}
