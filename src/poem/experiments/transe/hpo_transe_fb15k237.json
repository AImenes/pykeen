{
  "metadata": {
    "title": "HPO Over Nations for TransE"
  },
  "pipeline": {
    "dataset": "fb15k237",
    "dataset_kwargs": {
      "create_inverse_triples": true
    },
    "model": "TransE",
    "model_kwargs": {
      "scoring_fct_norm": 1
    },
    "model_kwargs_ranges": {
      "embedding_dim": {
        "type": "int",
        "low": 5,
        "high": 15
      }
    },
    "optimizer": "Adam",
    "optimizer_kwargs": {
    },
    "optimizer_kwargs_ranges": {
      "lr": {
        "type": "float",
        "low": 0.001,
        "high": 0.1,
        "scale": "log"
      }
    },
    "loss": "NegativeSamplingSelfAdversarialLoss",
    "loss_kwargs_ranges": {
      "adversarial_temperature": {
        "type": "float",
        "low": 0.5,
        "high": 2.0,
        "q": 0.5
      },
      "margin": {
        "type": "float",
        "low": 1,
        "high": 24.0,
        "q": 2.0
      }

    },
    "training_loop": "OWA",
    "negative_sampler": "basic",
    "negative_sampler_kwargs_ranges": {
      "num_negs_per_pos": {
        "type": "int",
        "low": 5,
        "high": 400,
        "q": 20
      }
    },
    "training_kwargs": {
      "batch_size": 1024
    },
    "training_kwargs_ranges": {
      "num_epochs": {
        "type": "int",
        "low": 500,
        "high": 1000,
        "q": 100
      }
    },
    "evaluator_kwargs": {
      "filtered": true
    },
    "evaluation_kwargs": {
      "batch_size": 32
    }
  },
  "optuna": {
    "n_trials": 500,
    "timeout":  259200,
    "metric": "hits@10",
    "direction": "maximize",
    "sampler": "TPE"
  }
}
