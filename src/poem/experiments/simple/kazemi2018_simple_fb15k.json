{
  "metadata": {
    "title": "Learn FB15k Data Set with SimplE as described by Kazemi et al., 2018",
    "comments": ""

  },
  "pipeline": {
    "data_set": "fb15k",
    "model": "SimplE",
    "model_kwargs": {
      "embedding_dim": 200,
      "regularization_weight": 0.1
    },
    "optimizer": "Adagrad",
    "optimizer_kwargs": {
      "lr": 0.05
    },
    "criterion": "SoftplusLoss",
    "early_stopping": true,
    "early_stopping_kwargs": {
      "metric": "mean_rank",
      "frequency": 50
    },
    "training_loop": "OWA",
    "training_kwargs": {
      "num_epochs": 1000,
      "batch_size": 4831,
      "negative_sampler_cls": "BasicNegativeSampler",
      "num_negs_per_pos": 10
    }
  }
}
