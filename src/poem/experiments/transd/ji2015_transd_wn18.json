{
  "metadata": {
    "title": "Learn WN18 Data Set with TransD as described by Ji et al., 2015"
  },
  "pipeline": {
    "dataset": "wn18",
    "model": "TransD",
    "model_kwargs": {
      "embedding_dim": 50,
      "relation_dim": 50
    },
    "optimizer": "Adadelta",
    "loss": "MarginRankingLoss",
    "loss_kwargs": {
      "margin": 1
    },
    "training_loop": "OWA",
    "negative_sampler": "bernoulli",
    "negative_sampler_kwargs": {
      "num_negs_per_pos": 1
    },
    "training_kwargs": {
      "num_epochs": 1000,
      "batch_size": 200
    },
    "evaluator_kwargs": {
      "filtered": true
    },
    "evaluation_kwargs": {
      "batch_size": 16
    }
  }
}
