{
  "metadata": {
    "title": "Learn FB15K Data Set with ComplEx as described by Trouillon et al., 2016"
  },
  "pipeline": {
    "data_set": "fb15k",
    "model": "ComplEx",
    "model_kwargs": {
      "embedding_dim": 200,
      "regularization_weight": 0.01
    },
    "optimizer": "AdaGrad",
    "optimizer_kwargs": {
      "lr": 0.5
    },
    "criterion": "SoftplusLoss",
    "early_stopping": true,
    "early_stopping_kwargs": {
      "metric": "mean_rank",
      "frequency": 50
    },
    "training_loop": "OWA",
    "training_kwargs": {
      "num_epochs": 1000,
      "batch_size": 4831,
      "negative_sampler_cls": "BasicNegativeSampler",
      "num_negs_per_pos": 10
    }
  }
}
